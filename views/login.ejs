<h1>Login</h1>

<div ng-controller="loginController">
  <div class="col-md-6">
    <form>
      <div class="form-group">
        <label>Email</label>
        <input class="form-control" ng-model="email" type="email"></input>
      </div>
      <div class="form-group">
        <label>Password</label>
        <input class="form-control" ng-model="password" type="password"></input>
      </div>
      <div><button class="btn-primary" ng-click="login()">Log In</div>
    </form>
    <p><br />Don't have an account? <a href="/register">Sign up now</a>!</p>
  </div>
</div>
