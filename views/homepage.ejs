

<!-- articles -->

<% for(var i=0; i<articles.length; i++) { %>

  <a href="/article/<%- articles[i].id %>">
<div class="panel panel-default">
  <div class="panel-body">
   <div style="float: left; font-size:20px; color:black">
    <%- articles[i].title %>
   </div>

   <div style="float: right; font-size:20px; color:black">
   <%- articles[i].link %>
   </div>

   <div style="clear: both;"></div>
  </div>
</div>
  </a>
<% } %>

